<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="me.weicools.material.component.expansion.panel">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/bottom_navigation_ic_launcher"
        android:label="@string/expansion_panel_app_name"
        android:roundIcon="@mipmap/bottom_navigation_ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".ExpansionPanelSummaryActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".ExpansionPanelBasicActivity" />
        <activity android:name=".ExpansionPanelInvoiceActivity" />
        <activity android:name=".ExpansionPanelTicketActivity" />
    </application>
</manifest>
