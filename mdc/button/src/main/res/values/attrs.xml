<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MaterialButton">
        <attr name="mdb_Background" format="reference" />

        <attr name="mdb_Ripple_enable" format="boolean" />
        <attr name="mdb_Ripple_color" format="color" />

        <attr name="mdb_Solid_color" format="color" />

        <attr name="mdb_Gradient_startColor" format="color" />
        <attr name="mdb_Gradient_centerColor" format="color" />
        <attr name="mdb_Gradient_endColor" format="color" />
        <attr name="mdb_Gradient_orientation" format="enum">
            <enum name="TOP_BOTTOM" value="0" />
            <enum name="BOTTOM_TOP" value="1" />
            <enum name="LEFT_RIGHT" value="2" />
            <enum name="RIGHT_LEFT" value="3" />
            <enum name="TL_BR" value="4" />
            <enum name="TR_BL" value="5" />
            <enum name="BL_TR" value="6" />
            <enum name="BR_TL" value="7" />
        </attr>

        <attr name="mdb_Stroke_color" format="color" />
        <attr name="mdb_Stroke_width" format="dimension" />
        <attr name="mdb_Stroke_dashGap" format="dimension" />
        <attr name="mdb_Stroke_dashWidth" format="dimension" />

        <attr name="mdb_Corner_radius" format="dimension" />
        <attr name="mdb_Corner_topLeftRadius" format="dimension" />
        <attr name="mdb_Corner_topRightRadius" format="dimension" />
        <attr name="mdb_Corner_bottomLeftRadius" format="dimension" />
        <attr name="mdb_Corner_bottomRightRadius" format="dimension" />
    </declare-styleable>
</resources>